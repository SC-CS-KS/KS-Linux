\par \plain \f0\fs18 /etc/vsftpd,conf \uc2\u27880 \'d7\'a2\u37322 \'ca\'cd\u25481 \'b5\'f4listen_ipv6=YES    \u35774 \'c9\'e8\u32622 \'d6\'c3listen=NO\uc1
\par #service xinetd restart
\par ftp\uc2\u26080 \'ce\'de\u27861 \'b7\'a8\u36830 \'c1\'ac\u25509 \'bd\'d3\uc1
