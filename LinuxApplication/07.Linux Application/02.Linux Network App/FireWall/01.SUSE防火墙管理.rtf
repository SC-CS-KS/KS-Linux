\par #!/bin/bash
\par # This program is used to use start my iptables.
\par PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:~/bin
\par export PATH
\par case \ldblquote $1\uc2\u8243 \'a1\'e5 in\uc1
\par start)
\par echo -n \ldblquote Staring FireWall \uc2\u8230 \'a1\'ad\rdblquote \uc1
\par # /sbin/iptables -P INPUT DROP
\par /sbin/iptables -P OUTPUT ACCEPT
\par /sbin/iptables -A INPUT -i lo -j ACCEPT
\par /sbin/iptables -A INPUT -p tcp -m tcp \endash dport 22 -j ACCEPT
\par /sbin/iptables -A INPUT -p tcp -m tcp \endash dport 80 -j ACCEPT
\par /sbin/iptables -A INPUT -p all -m state \endash state ESTABLISHED,RELATED -j ACCEPT
\par /sbin/iptables -A INPUT -p all -m state \endash state INVALID,NEW -j DROP
\par echo \ldblquote OK\rdblquote 
\par ;;
\par stop)
\par echo -n \ldblquote Stop FireWall \uc2\u8230 \'a1\'ad \rdblquote \uc1
\par /sbin/iptables -F
\par /sbin/iptables -X
\par /sbin/iptables -Z
\par echo \ldblquote OK\rdblquote 
\par ;;
\par restart)
\par /etc/init.d/firewall stop
\par /etc/init.d/firewall start
\par 
\par echo \ldblquote Restart FireWall OK\rdblquote 
\par ;;
\par *)
\par echo \ldblquote Usage: $0 \{start|stop|restart\}\rdblquote 
\par esac
\par 
