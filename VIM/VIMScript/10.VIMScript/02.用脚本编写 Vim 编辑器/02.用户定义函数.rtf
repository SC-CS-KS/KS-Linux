\par \plain \f1\fs20 function AlignAssignments ()\'0a 
\par \plain \f1\i\fs20     "Patterns needed to locate assignment operators...
\par \plain \f1\fs20     let ASSIGN_OP   = '[-+*/%|&]\\?=\\@<!=[=~]\\@!'\'0a 
\par     let ASSIGN_LINE = '^\\(.\\\{-\}\\)\\s*\\(' . ASSIGN_OP . '\\)'\'0a\'0a
\par 
\par \plain \f1\i\fs20     "Locate block of code to be considered (same indentation, no blanks)
\par \plain \f1\fs20     let indent_pat = '^' . matchstr(getline('.'), '^\\s*') . '\\S'\'0a 
\par     let firstline  = search('^\\%('. indent_pat . '\\)\\@!','bnW') + 1\'0a 
\par     let lastline   = search('^\\%('. indent_pat . '\\)\\@!', 'nW') - 1\'0a 
\par     if lastline < 0\'0a 
\par         let lastline = line('$')\'0a 
\par     endif\'0a\'0a
\par 
\par \plain \f1\i\fs20     "Find the column at which the operators should be aligned...
\par \plain \f1\fs20     let max_align_col = 0\'0a 
\par     let max_op_width  = 0\'0a 
\par     for linetext in getline(firstline, lastline)\'0a 
\par \plain \f1\i\fs20         "Does this line have an assignment in it?
\par \plain \f1\fs20         let left_width = match(linetext, '\\s*' . ASSIGN_OP)\'0a\'0a
\par 
\par \plain \f1\i\fs20         "If so, track the maximal assignment column and operator width...
\par \plain \f1\fs20         if left_width >= 0\'0a 
\par             let max_align_col = max([max_align_col, left_width])\'0a\'0a
\par 
\par             let op_width      = strlen(matchstr(linetext, ASSIGN_OP))\'0a 
\par             let max_op_width  = max([max_op_width, op_width+1])\'0a 
\par          endif\'0a 
\par     endfor\'0a\'0a
\par 
\par \plain \f1\i\fs20     "Code needed to reformat lines so as to align operators...
\par \plain \f1\fs20     let FORMATTER = '\\=printf("%-*s%*s", max_align_col, submatch(1),\'0a 
\par     \\                                    max_op_width,  submatch(2))'\'0a\'0a
\par 
\par \plain \f1\i\fs20     " Reformat lines with operators aligned in the appropriate column...
\par \plain \f1\fs20     for linenum in range(firstline, lastline)\'0a 
\par         let oldline = getline(linenum)\'0a 
\par         let newline = substitute(oldline, ASSIGN_LINE, FORMATTER, "")\'0a 
\par         call setline(linenum, newline)\'0a 
\par     endfor\'0ae
\par endfunction\'0a\'0a
\par \'0an
