\par function! SmartComplete ()\'0a 
\par \plain \f1\i\fs20     " Remember where we parked...
\par \plain \f1\fs20     let cursorpos = getpos('.')\'0a 
\par     let cursorcol = cursorpos[2]\'0a 
\par     let curr_line = getline('.')\'0a\'0a
\par 
\par \plain \f1\i\fs20     " Special subpattern to match only at cursor position...
\par \plain \f1\fs20     let curr_pos_pat = '\\%' . cursorcol . 'c'\'0a\'0a
\par 
\par \plain \f1\i\fs20     " Tab as usual at the left margin...
\par \plain \f1\fs20     if curr_line =~ '^\\s*' . curr_pos_pat\'0a 
\par         return "\\<TAB>"\'0a 
\par     endif\'0a\'0a
\par 
\par \plain \f1\i\fs20     " How to restore the cursor position...
\par \plain \f1\fs20     let cursor_back = "\\<C-O>:call setpos('.'," . string(cursorpos) . ")\\<CR>"\'0a\'0a
\par 
\par \plain \f1\i\fs20     " If a matching smart completion has been specified, use that...
\par \plain \f1\fs20     for [left, right, completion, restore] in s:completions\'0a 
